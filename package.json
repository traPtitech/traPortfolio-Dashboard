{
  "name": "traportfolio-dashboard",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run start-mock\"",
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --fix --cache --ext .ts,.vue src",
    "lint:nofix": "eslint --cache --ext .ts,.vue src",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:nofix": "prettier --cache --check \"src/**/*.{vue,ts}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest --passWithNoTests",
    "gen-api": "node scripts/generateApi.js",
    "start-mock": "prism mock -p 4010 -d https://raw.githubusercontent.com/traPtitech/traPortfolio/main/docs/swagger/traPortfolio.v1.yaml"
  },
  "dependencies": {
    "autosize": "^6.0.1",
    "axios": "^1.7.7",
    "pinia": "^2.2.4",
    "ress": "^5.0.2",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5",
    "vue-select": "^4.0.0-beta.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.2",
    "@stoplight/prism-cli": "^5.10.0",
    "@types/autosize": "^4.0.3",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.2.5",
    "sass": "^1.79.4",
    "ts-morph": "^23.0.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "vue-tsc": "2.1.6"
  },
  "private": true,
  "type": "module"
}
