{
  "name": "traportfolio-dashboard",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run start-mock\"",
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --fix --cache --ext .ts,.vue src",
    "lint:nofix": "eslint --cache --ext .ts,.vue src",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:nofix": "prettier --cache --check \"src/**/*.{vue,ts}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest --passWithNoTests",
    "gen-api": "node scripts/generateApi.cjs",
    "start-mock": "prism mock -p 4010 -d https://raw.githubusercontent.com/traPtitech/traPortfolio/main/docs/swagger/traPortfolio.v1.yaml",
    "postinstall": "npm run gen-api"
  },
  "dependencies": {
    "@iconify/iconify": "^3.1.1",
    "autosize": "^6.0.1",
    "axios": "^1.6.8",
    "pinia": "^2.1.6",
    "ress": "^5.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vue-select": "^4.0.0-beta.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.196",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@stoplight/prism-cli": "^5.3.2",
    "@types/autosize": "^4.0.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.25.0",
    "node-fetch": "^2.6.7",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.0",
    "ts-morph": "^20.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.7",
    "vite-plugin-purge-icons": "^0.10.0",
    "vitest": "^1.2.2",
    "vue-tsc": "1.8.0"
  },
  "private": true,
  "type": "module"
}
