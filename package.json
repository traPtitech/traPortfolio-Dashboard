{
  "name": "traportfolio-dashboard",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --fix --cache src",
    "lint:nofix": "eslint --cache src",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:nofix": "prettier --cache --check \"src/**/*.{vue,ts}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest --passWithNoTests",
    "gen-api": "node scripts/generateApi.js",
    "start-mock": "prism mock -p 4010 -d https://raw.githubusercontent.com/traPtitech/traPortfolio/main/docs/swagger/traPortfolio.v1.yaml"
  },
  "dependencies": {
    "autosize": "^6.0.1",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "ress": "^5.0.2",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1",
    "vue-select": "^4.0.0-beta.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@iconify/vue": "^5.0.0",
    "@types/autosize": "^4.0.3",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "ts-morph": "^26.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vue-tsc": "3.0.6"
  },
  "private": true,
  "type": "module"
}
