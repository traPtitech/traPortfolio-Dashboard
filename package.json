{
  "name": "traportfolio-dashboard",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run start-mock\"",
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --fix --cache --ext .ts,.vue src",
    "lint:nofix": "eslint --cache --ext .ts,.vue src",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:nofix": "prettier --cache --check \"src/**/*.{vue,ts}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest --passWithNoTests",
    "gen-api": "node scripts/generateApi.js",
    "start-mock": "prism mock -p 4010 -d https://raw.githubusercontent.com/traPtitech/traPortfolio/main/docs/swagger/traPortfolio.v1.yaml"
  },
  "dependencies": {
    "@iconify/iconify": "^3.1.1",
    "autosize": "^6.0.1",
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "ress": "^5.0.2",
    "vue": "^3.4.27",
    "vue-router": "^4.4.0",
    "vue-select": "^4.0.0-beta.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.215",
    "@stoplight/prism-cli": "^5.8.1",
    "@types/autosize": "^4.0.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "node-fetch": "^2.6.7",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "ts-morph": "^20.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-purge-icons": "^0.10.0",
    "vitest": "^1.5.3",
    "vue-tsc": "2.0.19"
  },
  "private": true,
  "type": "module"
}
